{
    "name": "dorvidas/laravel-query-builder",
    "description": "Easily build Eloquent queries from API requests",
    "keywords": [
        "json api",
        "laravel-query-builder"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Darius Orvidas",
            "email": "dorvidas@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1|^7.0",
        "orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0"
    },
    "autoload": {
        "psr-4": {
            "Dorvidas\\QueryBuilder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dorvidas\\QueryBuilder\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dorvidas\\QueryBuilder\\QueryBuilderServiceProvider"
            ]
        }
    }
}
