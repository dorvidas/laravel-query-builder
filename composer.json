{
  "name": "dorvidas/laravel-query-builder",
  "description": "Easily build Eloquent queries from API requests",
  "keywords": [
    "json api",
    "laravel-query-builder"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Darius Orvidas",
      "email": "dorvidas@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1",
    "illuminate/database": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
    "illuminate/http": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
    "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
    "rapidwebltd/array_undot": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0|^8.0",
    "orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0|~3.8.0",
    "mockery/mockery": "^1.0.0"
  },
  "autoload": {
    "psr-4": {
      "Dorvidas\\QueryBuilder\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dorvidas\\QueryBuilder\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "phpunit --coverage-html coverage"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Dorvidas\\QueryBuilder\\QueryBuilderServiceProvider"
      ]
    }
  }
}
